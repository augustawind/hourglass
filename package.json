{
  "name": "hourglass",
  "version": "0.0.1",
  "description": "CLI time management tool.",
  "bin": "bin/hourglass.js",
  "main": "lib/main.js",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib/*",
    "build": "babel src/ -d lib/",
    "build-dev": "npm run build -- --source-maps",
    "watch": "npm run build-dev -- --watch",
    "lint": "standard src/*.js bin/*.js",
    "test": "blue-tape -r babel-register test/**/*.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinrohde/hourglass.git"
  },
  "keywords": [
    "time",
    "management",
    "productivity",
    "cli"
  ],
  "author": "Dustin Rohde",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dustinrohde/hourglass/issues"
  },
  "homepage": "https://github.com/dustinrohde/hourglass#readme",
  "babel": {
    "presets": [
      "es2015-loose"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-register": "6.6.5",
    "blue-tape": "0.2.0",
    "faucet": "0.0.1",
    "rimraf": "2.5.2",
    "standard": "6.0.8",
    "test-console": "1.0.0"
  },
  "dependencies": {
    "commander": "2.9.0",
    "lame": "1.2.3",
    "lodash": "4.6.1",
    "mz": "2.4.0",
    "prettyjson": "1.1.3",
    "progress": "1.1.8",
    "source-map-support": "0.4.0",
    "speaker": "0.2.6"
  }
}
